-- BotÃ£o de Anti-AFK
ScriptsTab:AddButton({
    Name = "🚸 ANTI-AFK",
    Callback = function()
        wait(0.5)
        local gui = Instance.new("ScreenGui")
        local title = Instance.new("TextLabel")
        local frame = Instance.new("Frame")
        local footer = Instance.new("TextLabel")
        local status = Instance.new("TextLabel")

        gui.Parent = game.CoreGui
        gui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

        title.Parent = gui
        title.Active = true
        title.BackgroundColor3 = Color3.new(0.5, 0, 0)
        title.Draggable = true
        title.Position = UDim2.new(0.7, 0, 0.1, 0)
        title.Size = UDim2.new(0, 370, 0, 52)
        title.Font = Enum.Font.SourceSansSemibold
        title.Text = "🚸 ANTI-AFK"
        title.TextColor3 = Color3.new(1, 0, 0)
        title.TextSize = 22

        frame.Parent = title
        frame.BackgroundColor3 = Color3.new(0.6, 0, 0)
        frame.Position = UDim2.new(0, 0, 1.02, 0)
        frame.Size = UDim2.new(0, 370, 0, 107)

        footer.Parent = frame
        footer.BackgroundColor3 = Color3.new(0.5, 0, 0)
        footer.Position = UDim2.new(0, 0, 0.8, 0)
        footer.Size = UDim2.new(0, 370, 0, 21)
        footer.Font = Enum.Font.Arial
        footer.Text = "YKS ANTI AFK"
        footer.TextColor3 = Color3.new(1, 0, 0)
        footer.TextSize = 20

        status.Parent = frame
        status.BackgroundColor3 = Color3.new(0.5, 0, 0)
        status.Position = UDim2.new(0, 0, 0.16, 0)
        status.Size = UDim2.new(0, 370, 0, 44)
        status.Font = Enum.Font.ArialBold
        status.Text = "Status: Ativo"
        status.TextColor3 = Color3.new(1, 0, 0)
        status.TextSize = 20

        local vu = game:GetService("VirtualUser")
        game:GetService("Players").LocalPlayer.Idled:Connect(function()
            vu:CaptureController()
            vu:ClickButton2(Vector2.new())
            status.Text = "Roblox doesn't get me hahaha"
            wait(2)
            status.Text = "Status: Ativo"
        end)
    end
})

