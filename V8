   local farm = Window:MakeTab({
        Name = "farm",
        Icon = "rbxassetid://132965427906211",
        PremiumOnly = false
    })

-- Adicionando o Toggle na aba "farm"
farm:AddToggle({
    Name = "🌝 Farm Dummy",
    Default = false, -- Valor inicial
    Callback = function(Value)
        FarmDummy = Value -- Atualiza a variável FarmDummy com o valor do Toggle
 
        if FarmDummy then
            -- Toca o som
            local sound = Instance.new("Sound")
            sound.SoundId = "rbxassetid://7269900245"
            sound.Parent = game.Players.LocalPlayer.Character.HumanoidRootPart
            sound:Play()
 
            -- Inicia o loop para o farm
            task.spawn(function()
                while FarmDummy do
                    pcall(function()
                        local Dummy = workspace.MAP.dummies:FindFirstChild("Dummy")
                        if Dummy and Dummy:FindFirstChild("HumanoidRootPart") then
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Dummy.HumanoidRootPart.CFrame
                        end
                        if Dummy and Dummy:FindFirstChild("Humanoid") then
                            game:GetService("ReplicatedStorage").jdskhfsIIIllliiIIIdchgdIiIIIlIlIli:FireServer(Dummy.Humanoid, 1)
                        end
                    end)
                    task.wait()
                end
            end)

            -- Ativa o hitting alternando entre NewFireball e NewLightningball
            task.spawn(function()
                local toggle = true -- Alternador entre habilidades
                while FarmDummy do
                    local skillName = toggle and "NewFireball" or "NewLightningball" -- Alterna entre os ataques
                    local skillArgs = {
                        Vector3.new(-126.10099792480469, 646.1202392578125, 594.0977783203125),
                        skillName
                    }
                    game:GetService("ReplicatedStorage").SkillsInRS.RemoteEvent:FireServer(unpack(skillArgs))

                    toggle = not toggle -- Alterna para o próximo ataque na próxima execução
                    task.wait() -- Pequeno delay antes de repetir
                end
            end)

        else
            -- Quando desativado, cancela o farm e o hitting
            print("Farm Dummy desativado!")
        end
    end
})

-- Criando a opÃ§Ã£o "ðŸ’« Auto Farm Dummy 5k"
farm:AddToggle({
    Name = "🌚 Auto Farm Dummy 5k",
    CurrentValue = AutoFarmDummy5k,
    Callback = function(Value)
        AutoFarmDummy5k = Value

        if AutoFarmDummy5k then
            -- Toca o som quando o farm é ativado
            local sound = Instance.new("Sound")
            sound.SoundId = "rbxassetid://7269900245"
            sound.Parent = game.Players.LocalPlayer.Character.HumanoidRootPart
            sound:Play()

            -- Inicia o loop de farm e hitting
            task.spawn(function()
                while AutoFarmDummy5k do
                    pcall(function()
                        local Dummy2 = workspace.MAP["5k_dummies"]:FindFirstChild("Dummy2")
                        if Dummy2 and Dummy2:FindFirstChild("HumanoidRootPart") then
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Dummy2.HumanoidRootPart.CFrame
                        end
                        if Dummy2 and Dummy2:FindFirstChild("Humanoid") then
                            game:GetService("ReplicatedStorage").jdskhfsIIIllliiIIIdchgdIiIIIlIlIli:FireServer(Dummy2.Humanoid, 2)
                        end
                    end)

                    -- Hitting (Dispara os dois ataques um após o outro)
                    local fireballArgs = {
                        Vector3.new(-83.04418182373047, 595.0643920898438, 813.4301147460938),
                        "NewFireball"
                    }
                    local lightningArgs = {
                        Vector3.new(-83.04418182373047, 595.0643920898438, 813.4301147460938),
                        "NewLightningball"
                    }

                    game:GetService("ReplicatedStorage").SkillsInRS.RemoteEvent:FireServer(unpack(fireballArgs))
                    task.wait(0.1) -- Pequeno delay antes do segundo ataque
                    game:GetService("ReplicatedStorage").SkillsInRS.RemoteEvent:FireServer(unpack(lightningArgs))

                    task.wait(0.1) -- Espera entre os loops
                end
            end)

        else
            print("🫀 Auto Farm Dummy 5k desativado!")
        end
    end
})

