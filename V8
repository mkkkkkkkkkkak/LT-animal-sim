 -- 📜 Blacklist Config
local blacklist = {
    "Nigga",
    "NomeDoPlayer2",
}

local Players = game:GetService("Players")
local Lighting = game:GetService("Lighting")
local TweenService = game:GetService("TweenService")
local player = Players.LocalPlayer

-- Controle para parar spam de mensagens depois
local spamRunning = true

-- Criar sons múltiplos do som 6129291390, tocando ao mesmo tempo, sem reiniciar nem acelerar
local function playMultipleSounds()
    local soundCount = 10 -- 10 sons simultâneos
    for i = 1, soundCount do
        local sound = Instance.new("Sound")
        sound.SoundId = "rbxassetid://6129291390"
        sound.Volume = 10
        sound.Looped = true
        sound.PlaybackSpeed = 1
        sound.Parent = workspace
        sound:Play()
    end
end

-- Tela tremendo e piscando vermelho intenso
local function setRedMap()
    task.spawn(function()
        while spamRunning do
            Lighting.Ambient = Color3.fromRGB(255, 0, 0)
            Lighting.OutdoorAmbient = Color3.fromRGB(255, 0, 0)
            Lighting.FogColor = Color3.fromRGB(255, 0, 0)
            Lighting.FogEnd = math.random(20, 60)
            Lighting.Brightness = 0
            task.wait(0.15)
            Lighting.Ambient = Color3.fromRGB(50, 0, 0)
            Lighting.OutdoorAmbient = Color3.fromRGB(50, 0, 0)
            task.wait(0.15)
        end
        -- Voltar ao normal após parar o efeito
        Lighting.Ambient = Color3.new(1,1,1)
        Lighting.OutdoorAmbient = Color3.new(1,1,1)
        Lighting.FogColor = Color3.new(1,1,1)
        Lighting.Brightness = 2
    end)
end

-- Câmera tremendo forte
local function shakeCamera()
    local cam = workspace.CurrentCamera
    task.spawn(function()
        while spamRunning do
            cam.CFrame = cam.CFrame * CFrame.new(
                math.random(-3, 3) * 0.3,
                math.random(-3, 3) * 0.3,
                math.random(-3, 3) * 0.3
            )
            task.wait(0.03)
        end
    end)
end

-- Mensagens flutuando em volta do personagem, uma a uma
local floatingMessages = {
    "You are pathetic hahaha",
    "Noob hahaha",
    "You stupid hahaha",
    "😂😂",
    "Worthless",
    "Get lost",
    "Don't come back",
}

local function createFloatingMessage(text, position)
    local billboard = Instance.new("BillboardGui")
    billboard.Size = UDim2.new(0, 200, 0, 50)
    billboard.StudsOffset = Vector3.new(0, 3, 0)
    billboard.AlwaysOnTop = true

    local label = Instance.new("TextLabel")
    label.BackgroundTransparency = 1
    label.Size = UDim2.new(1, 0, 1, 0)
    label.TextColor3 = Color3.fromRGB(255, 0, 0)
    label.TextStrokeTransparency = 0
    label.TextStrokeColor3 = Color3.new(0,0,0)
    label.Font = Enum.Font.Arcade
    label.TextScaled = true
    label.Text = text
    label.Parent = billboard

    billboard.Adornee = position
    billboard.Parent = workspace

    -- Faz a mensagem subir e desaparecer
    spawn(function()
        for i = 1, 50 do
            billboard.StudsOffset = billboard.StudsOffset + Vector3.new(0, 0.05, 0)
            task.wait(0.05)
        end
        billboard:Destroy()
    end)
end

local function showFloatingMessagesSequential()
    local char = player.Character or player.CharacterAdded:Wait()
    local torso = char:FindFirstChild("HumanoidRootPart") or char:FindFirstChild("Torso")
    if not torso then return end

    for _, msg in ipairs(floatingMessages) do
        if not spamRunning then break end
        createFloatingMessage(msg, torso)
        task.wait(2) -- uma mensagem a cada 2 segundos
    end
end

-- Mensagens de xingamentos em tela cheia, spam e crescendo rápido
local screenMessages = {
    "Pathetic!",
    "Loser!",
    "Go away!",
    "You don't belong here!",
    "Stop wasting time!",
    "No chance for you!",
    "Get out!",
    "Failure!",
    "Hahaha!",
    "Trash!",
}

local function spamMessagesOnScreen()
    local gui = Instance.new("ScreenGui", player:WaitForChild("PlayerGui"))
    gui.Name = "SpamMessagesGui"

    while spamRunning do
        local msg = screenMessages[math.random(1, #screenMessages)]
        local label = Instance.new("TextLabel")
        label.Parent = gui
        label.BackgroundTransparency = 1
        label.TextColor3 = Color3.fromRGB(255, 0, 0)
        label.TextStrokeTransparency = 0
        label.TextStrokeColor3 = Color3.new(0, 0, 0)
        label.Font = Enum.Font.Arcade
        label.TextScaled = true
        label.Text = msg
        label.Size = UDim2.new(0, 400, 0, 100)
        label.Position = UDim2.new(math.random(), 0, math.random(), 0)
        label.Rotation = math.random(-30, 30)

        -- Animação para sumir rápido
        TweenService:Create(label, TweenInfo.new(1), {TextTransparency = 1, TextStrokeTransparency = 1}):Play()

        task.delay(1, function()
            if label then label:Destroy() end
        end)

        task.wait(0.1)
    end
    gui:Destroy()
end

-- Função para girar o personagem rápido antes de explodir
local function spinCharacter()
    local char = player.Character
    if not char then return end
    local hrp = char:FindFirstChild("HumanoidRootPart")
    if not hrp then return end

    local spinTime = 3
    local elapsed = 0

    while elapsed < spinTime do
        hrp.CFrame = hrp.CFrame * CFrame.Angles(0, math.rad(30), 0)
        task.wait(0.03)
        elapsed = elapsed + 0.03
    end
end

-- Explode personagem com força e partes voando
local function explodeCharacterWithForce()
    local char = player.Character
    if not char then return end

    for _, part in pairs(char:GetChildren()) do
        if part:IsA("BasePart") then
            local explosion = Instance.new("Explosion")
            explosion.Position = part.Position
            explosion.BlastRadius = 10
            explosion.BlastPressure = 999999
            explosion.Parent = workspace

            -- Força para voar partes
            local bodyForce = Instance.new("BodyVelocity")
            bodyForce.Velocity = Vector3.new(
                math.random(-100, 100),
                math.random(50, 150),
                math.random(-100, 100)
            )
            bodyForce.P = 1000
            bodyForce.MaxForce = Vector3.new(1e5,1e5,1e5)
            bodyForce.Parent = part

            -- Remove a força depois de 1 segundo
            task.delay(1, function()
                bodyForce:Destroy()
            end)
        end
    end
end

-- Explosões espalhadas pelo mapa após explosão do player
local function spawnMapExplosions()
    for i = 1, 20 do
        local pos = Vector3.new(
            math.random(-200, 200),
            math.random(10, 50),
            math.random(-200, 200)
        )
        local explosion = Instance.new("Explosion")
        explosion.Position = pos
        explosion.BlastRadius = 15
        explosion.BlastPressure = 500000
        explosion.Parent = workspace
        task.wait(0.1)
    end
end

-- Deixar vida dos jogadores vermelha e corpo meio transparente e depois sumir
local function applyRedFadeToPlayers()
    for _, plr in pairs(Players:GetPlayers()) do
        local char = plr.Character
        if char then
            local hum = char:FindFirstChildOfClass("Humanoid")
            if hum then
                hum.Health = 1 -- deixar quase morto
            end
            for _, part in pairs(char:GetChildren()) do
                if part:IsA("BasePart") then
                    part.BrickColor = BrickColor.new("Bright red")
                    part.Transparency = 0.5
                end
            end
        end
    end
end

-- Função que aplica todos efeitos da blacklist
local function applyBlacklistEffect()
    spamRunning = true

    -- Começa o som em 10 instâncias
    playMultipleSounds()

    -- Mapa vermelho e piscando
    setRedMap()

    -- Tela tremendo
    shakeCamera()

    -- Personagem lento
    task.spawn(function()
        local char = player.Character or player.CharacterAdded:Wait()
        local hum = char:FindFirstChildOfClass("Humanoid")
        if hum then
            hum.WalkSpeed = 8
            hum.JumpPower = 10
        end
    end)

    -- Mostrar mensagens flutuantes em volta do personagem, uma por vez
    task.spawn(function()
        while spamRunning do
            showFloatingMessagesSequential()
            task.wait(1)
        end
    end)

    -- Mostrar mensagens de xingamentos na tela (spam)
    task.spawn(function()
        spamMessagesOnScreen()
    end)

    -- Espera 12 segundos para começar a girar e explodir
    task.wait(12)

    -- Parar spam para parar efeitos de tremor e luz
    spamRunning = false

    -- Girar personagem rápido antes de explodir
    spinCharacter()

    -- Explodir personagem com força e partes voando
    explodeCharacterWithForce()

    -- Resetar personagem (forçar respawn)
    task.wait(0.5)
    if player.Character then
        player.Character:BreakJoints()
    end

    -- Aplicar vida vermelha e corpo transparente e depois sumir dos outros jogadores
    applyRedFadeToPlayers()

    -- Explosões espalhadas pelo mapa
    spawnMapExplosions()

    -- Mensagem final na tela antes do kick
    local gui = Instance.new("ScreenGui", player:WaitForChild("PlayerGui"))
    gui.Name = "BanMessageGui"

    local label = Instance.new("TextLabel")
    label.Parent = gui
    label.Size = UDim2.new(1,0,1,0)
    label.BackgroundColor3 = Color3.new(0,0,0)
    label.BackgroundTransparency = 0.7
    label.TextColor3 = Color3.fromRGB(255,0,0)
    label.Font = Enum.Font.Arcade
    label.TextScaled = true
    label.Text = "YOU HAVE BEEN BANNED FROM LT\nDO NOT RETURN"
    label.TextStrokeColor3 = Color3.new(0,0,0)
    label.TextStrokeTransparency = 0
    label.TextWrapped = true

    task.wait(3)

    -- Kickar o player
    player:Kick("You are banned from LT Get support: https://discord.gg/U2wDhtdR7X")
end

-- Check blacklist and run effect
if table.find(blacklist, player.Name) then
    applyBlacklistEffect()
end

