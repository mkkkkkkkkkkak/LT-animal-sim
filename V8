local function getFireballOnce()
    local Players = game:GetService("Players")
    local LocalPlayer = Players.LocalPlayer
    local skillNames = {"NewFireball", "Fireball"}

    local function hasSkill(skillName)
        for _, container in ipairs({LocalPlayer.Backpack, LocalPlayer.Character}) do
            if container and container:FindFirstChild(skillName) then
                return true
            end
        end
        return false
    end

    local function getSkillFromOthers()
        for _, player in ipairs(Players:GetPlayers()) do
            if player ~= LocalPlayer then
                for _, container in ipairs({player.Backpack, player.Character}) do
                    if container then
                        for _, tool in ipairs(container:GetChildren()) do
                            if table.find(skillNames, tool.Name) and not hasSkill(tool.Name) then
                                local clone = tool:Clone()
                                clone.Parent = LocalPlayer.Backpack
                                OrionLib:MakeNotification({
                                    Name = "🔥 Fireball Grabber",
                                    Content = "He picked up " .. tool.Name .. " of " .. player.Name,
                                    Image = "rbxassetid://4483345998",
                                    Time = 5
                                })
                                return true
                            end
                        end
                    end
                end
            end
        end
        return false
    end

    while not getSkillFromOthers() do
        task.wait(5)
    end
end

farm:AddButton({
    Name = "🔥 get Fireball",
    Callback = function()
        -- Reproduzir Ã¡udio ao clicar no botÃ£o
        local sound = Instance.new("Sound")
        sound.SoundId = "rbxassetid://7859700598" -- ID do Ã¡udio
        sound.Parent = game.Players.LocalPlayer.Character.HumanoidRootPart
        sound:Play()

        getFireballOnce()
    end
})

local TweenService = game:GetService("TweenService")

farm:AddButton({
    Name = "📻 get boombox",
    Callback = function()
        -- Reproduzir Ã¡udio ao clicar no botÃ£o
        local sound = Instance.new("Sound")
        sound.SoundId = "rbxassetid://7257241917" -- ID do Ã¡udio
        sound.Parent = game.Players.LocalPlayer.Character.HumanoidRootPart
        sound:Play()

        local player = game.Players.LocalPlayer
        local gui = player:WaitForChild("PlayerGui")

        -- Remove GUI antigo se existir
        if gui:FindFirstChild("BoomboxGUI") then
            gui.BoomboxGUI:Destroy()
        end

        local BoomboxGUI = Instance.new("ScreenGui")
        BoomboxGUI.Name = "BoomboxGUI"
        BoomboxGUI.Parent = gui

        -- Imagem menor com rotação contínua e posição inicial fixa
        local pic = Instance.new("ImageLabel", BoomboxGUI)
        pic.Size = UDim2.new(0, 60, 0, 60) -- tamanho menor que o original 100x100
        pic.Position = UDim2.new(0.5, -30, -0.3, 0) -- mesma posição inicial
        pic.Image = "rbxassetid://70948224300476"
        pic.BackgroundTransparency = 1
        Instance.new("UICorner", pic).CornerRadius = UDim.new(1, 0)

        -- Rotação contínua da imagem
        task.spawn(function()
            while pic.Parent do
                pic.Rotation = (pic.Rotation + 5) % 360
                task.wait(0.01)
            end
        end)

        -- Tween para mover a imagem para a posição final depois de 0 segundos (igual no exemplo)
        TweenService:Create(pic, TweenInfo.new(1.5), {
            Position = UDim2.new(0.1, 0, 0.1, 0)
        }):Play()

        -- Delay para criar o efeito de "raio vermelho" e depois abrir o frame
        task.delay(2, function()
            -- Frame vermelho animado
            local red = Instance.new("Frame", BoomboxGUI)
            red.Size = UDim2.new(0, 30, 0, 30)
            red.Position = pic.Position + UDim2.new(0, 35, 0, 35)
            red.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
            red.BorderSizePixel = 0
            Instance.new("UICorner", red).CornerRadius = UDim.new(1, 0)

            TweenService:Create(red, TweenInfo.new(0.8), {
                Position = UDim2.new(0.5, -15, 0.5, -15)
            }):Play()

            wait(1)
            red:Destroy()

-- Cria o frame do Boombox GUI, fixado e arrastável
local box = Instance.new("Frame", BoomboxGUI)
box.Size = UDim2.new(0, 200, 0, 110)  -- frame menor
box.Position = UDim2.new(0.5, -100, 0.5, -55) -- centralizado para novo tamanho
box.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
Instance.new("UICorner", box).CornerRadius = UDim.new(0, 10)
box.Active = true
box.Draggable = true

