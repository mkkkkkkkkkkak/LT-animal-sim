-- Criando a opÃ§Ã£o "ðŸ’° Auto Farm Coins"
farm:AddToggle({
    Name = "💰 Auto Farm Coins",
    CurrentValue = AutoFarmCoins,
    Callback = function(Value)
        AutoFarmCoins = Value

        if AutoFarmCoins then
            -- Toca o som quando o farm de moedas Ã© ativado
            local sound = Instance.new("Sound")
            sound.SoundId = "rbxassetid://7269900245"
            sound.Parent = game.Players.LocalPlayer.Character.HumanoidRootPart
            sound:Play()

            -- Inicia o loop de farm de moedas
            spawn(function()
                while AutoFarmCoins do
                    pcall(function()
                        game:GetService("ReplicatedStorage").Events.CoinEvent:FireServer()
                    end)
                    wait()
                end
            end)
        end
    end
})

-- Criando a opÃ§Ã£o "ðŸ¦Š farm boss"
farm:AddToggle({
    Name = "🔱 Farm Boss",
    Default = false,
    Callback = function(value)
        isFarmingBoss = value
        if isFarmingBoss then
            -- Som ao ativar o farm
            local sound = Instance.new("Sound")
            sound.SoundId = "rbxassetid://7269900245"
            sound.Parent = HumanoidRootPart
            sound:Play()

            task.spawn(function()
                local ReplicatedStorage = game:GetService("ReplicatedStorage")
                local bosses = {
                    "Griffin", "CRABBOSS", "LavaGorilla",
                    "CENTAUR", "DragonGiraffe", "BOSSBEAR", "BOSSDEER"
                }
                while isFarmingBoss do
                    for _, name in ipairs(bosses) do
                        local boss = workspace:FindFirstChild("NPC") and workspace.NPC:FindFirstChild(name)
                        if boss and boss:FindFirstChild("Humanoid") and boss.Humanoid.Health > 0 then
                            while isFarmingBoss and boss.Humanoid.Health > 0 do
                                pcall(function()
                                    ReplicatedStorage.jdskhfsIIIllliiIIIdchgdIiIIIlIlIli:FireServer(boss.Humanoid, 999999)
                                end)
                                task.wait(0.1)
                            end
                        end
                    end
                    task.wait(0.2)
                end
            end)
        end
    end
})

farm:AddToggle({ 
    Name = "🔱 Farm BossV2", 
    CurrentValue = farmBoss, 
    Callback = function(Value) 
        Farmbssv2 = Value
        local Players = game:GetService("Players")
        local ReplicatedStorage = game:GetService("ReplicatedStorage")
        local LocalPlayer = Players.LocalPlayer
        local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
        local HumanoidRootPart = Character:WaitForChild("HumanoidRootPart")

        if Farmbssv2 then
            -- Som ao ativar o farm
            local sound = Instance.new("Sound")
            sound.SoundId = "rbxassetid://7269900245"
            sound.Parent = HumanoidRootPart
            sound:Play()

            task.spawn(function()
                local bossNames = { "Griffin", "CRABBOSS", "LavaGorilla", "CENTAUR", "DragonGiraffe", "BOSSDEER", "BOSSBEAR" }
                local npcFolder = workspace:FindFirstChild("NPC")
                
                while Farmbssv2 do
                    local foundBoss = false
                    if npcFolder then
                        for _, bossName in ipairs(bossNames) do
                            local boss = npcFolder:FindFirstChild(bossName)
                            if boss then
                                local bossHRP = boss:FindFirstChild("HumanoidRootPart")
                                local bossHumanoid = boss:FindFirstChild("Humanoid")
                                if bossHRP and bossHumanoid and bossHumanoid.Health > 0 then
                                    foundBoss = true
                                    while bossHumanoid.Health > 0 and Farmbssv2 do
                                        local playerPos = HumanoidRootPart.Position
                                        
                                        -- Mantém o boss preso embaixo do jogador
                                        bossHRP.Anchored = true
                                        bossHRP.CFrame = CFrame.new(playerPos.X, playerPos.Y - 20, playerPos.Z)
                                        
                                        -- Aplica dano rápido (10x por ciclo)
                                        for _ = 1, 10 do
                                            pcall(function()
                                                ReplicatedStorage.jdskhfsIIIllliiIIIdchgdIiIIIlIlIli:FireServer(bossHumanoid, 1)
                                            end)
                                        end
                                        
                                        task.wait(0.05) -- intervalo menor para ser mais rápido
                                    end
                                end
                            end
                        end
                    end
                    if not foundBoss then
                        task.wait(1) -- espera um pouco se não achar boss vivo
                    else
                        task.wait(0.1) -- pequeno delay pra evitar uso excessivo
                    end
                end
            end)
        else
            print("Farm desativado!")
        end
    end 
})

farm:AddToggle({ 
    Name = "🌙 farm all",
    CurrentValue = false,
    Callback = function(Value)
        farmAll = Value

        local player = game.Players.LocalPlayer
        local ReplicatedStorage = game:GetService("ReplicatedStorage")

        if farmAll then
            local sound = Instance.new("Sound")
            sound.SoundId = "rbxassetid://7269900245"
            sound.Parent = player.Character and player.Character:FindFirstChild("HumanoidRootPart") or player:WaitForChild("PlayerGui")
            sound:Play()

            farmTask = task.spawn(function()
                local bosses = {
                    {name = "Griffin", damage = 10},     -- aumentei o dano
                    {name = "CRABBOSS", damage = 7},
                    {name = "LavaGorilla", damage = 15},
                    {name = "CENTAUR", damage = 12},
                    {name = "DragonGiraffe", damage = 7},
                    {name = "BOSSDEER", damage = 30},
                    {name = "BOSSBEAR", damage = 8},
                }

                while farmAll do
                    pcall(function()
                        -- Farm Coins
                        ReplicatedStorage.Events.CoinEvent:FireServer()

                        -- Farm Bosses com rajada de ataques
                        local npcFolder = workspace:FindFirstChild("NPC")
                        if npcFolder then
                            for _, boss in ipairs(bosses) do
                                local bossModel = npcFolder:FindFirstChild(boss.name)
                                local humanoid = bossModel and bossModel:FindFirstChild("Humanoid")
                                if humanoid and humanoid.Health > 0 then
                                    -- Manda uma rajada de 5 ataques por boss para matar mais rápido
                                    for i = 1, 5 do
                                        ReplicatedStorage.jdskhfsIIIllliiIIIdchgdIiIIIlIlIli:FireServer(humanoid, boss.damage)
                                    end
                                end
                            end
                        end

                        -- Skill hitting
                        ReplicatedStorage.SkillsInRS.RemoteEvent:FireServer(
                            Vector3.new(-83.04418182373047, 595.0643920898438, 813.4301147460938),
                            "NewFireball"
                        )
                    end)

                    task.wait(0.03) -- mais rápido ainda, sem travar
                end
            end)

        else
            if farmTask then
                task.cancel(farmTask)
                farmTask = nil
            end
        end
    end
})

