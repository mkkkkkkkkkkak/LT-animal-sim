local trollSection = troll:AddSection({
        Name = "Lt4xx 🚸"
    })

local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local LocalPlayer = Players.LocalPlayer

local skillNames = {"NewFireball", "Fireball"}
local running = false -- controle do toggle
local autoFireTask = nil -- para armazenar a thread do autoFire

local function hasSkill()
    for _, container in ipairs({LocalPlayer.Backpack, LocalPlayer.Character}) do
        if container then
            for _, tool in ipairs(container:GetChildren()) do
                if table.find(skillNames, tool.Name) then
                    return tool.Name
                end
            end
        end
    end
    return nil
end

local function stealSkillOnce()
    if hasSkill() then return end
    for _, player in ipairs(Players:GetPlayers()) do
        if player ~= LocalPlayer then
            for _, container in ipairs({player.Backpack, player.Character}) do
                if container then
                    for _, tool in ipairs(container:GetChildren()) do
                        if table.find(skillNames, tool.Name) then
                            tool:Clone().Parent = LocalPlayer.Backpack
                            print("🔥 Got skill:", tool.Name, "from", player.Name)
                            return
                        end
                    end
                end
            end
        end
    end
end

local function autoFire(skillName)
    local targetPos = Vector3.new(-81.1446, 596.1986, 813.1804)

    while running do
        task.wait(0.1)
        if not running then break end
        if hasSkill() == skillName then
            for i = 1, 10 do -- 10 disparos por ciclo (pode aumentar)
                ReplicatedStorage.SkillsInRS.RemoteEvent:FireServer(targetPos, skillName)
                if not running then break end
                task.wait(0.01)
            end
        else
            stealSkillOnce()
        end
    end
end

troll:AddToggle({
    Name = "🔥 bug Fireball",
    Default = false,
    Callback = function(Value)
        running = Value
        if running then
            stealSkillOnce()
            local skill = hasSkill()
            if skill then
                -- roda autoFire em thread separada pra não travar a UI
                autoFireTask = task.spawn(autoFire, skill)
            else
                autoFireTask = task.spawn(function()
                    while running and not hasSkill() do
                        stealSkillOnce()
                        task.wait(2)
                    end
                    if running then
                        autoFire(hasSkill())
                    end
                end)
            end
        else
            -- parar o autoFire (loop para ao checar running)
            if autoFireTask then
                autoFireTask = nil
            end
        end
    end
})

local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

local running = false
local fireTask = nil

local CarryEvent = ReplicatedStorage:WaitForChild("Events"):WaitForChild("CarryEvent")
local localPlayer = Players.LocalPlayer

local function autoCarry()
    while running do
        for _, player in ipairs(Players:GetPlayers()) do
            if player ~= localPlayer then  -- Ignora o próprio usuário
                local args = {
                    [1] = player,
                    [2] = "request_carry"
                }
                -- Multiplica disparos, por exemplo 50 vezes por ciclo
                for i = 1, 1000 do
                    CarryEvent:FireServer(unpack(args))
                    if not running then break end
                end
                if not running then break end
            end
        end
        task.wait(0.1) -- espera entre ciclos (controla velocidade)
    end
end

troll:AddToggle({
    Name = "🏴‍☠️ lag server",
    Default = false,
    Callback = function(Value)
        running = Value
        if running then
            fireTask = task.spawn(autoCarry)
        else
            fireTask = nil -- o loop termina porque running = false
        end
    end
})

troll:AddToggle({
    Name = "🚸 Spam Clan YK2",
    Default = false,
    Callback = function(Value)
        SpamClanYK2Active = Value -- VariÃ¡vel para controlar o estado da funÃ§Ã£o

        if SpamClanYK2Active then
            -- Som ao ativar
            local sound = Instance.new("Sound")
            sound.SoundId = "rbxassetid://7269900245"
            sound.Parent = game.Players.LocalPlayer.Character.HumanoidRootPart
            sound:Play()
            -- Lista de nomes do clÃ£ para alternar
            local clanNames = {
                "LT FRUITS",
                "LT ez",
                "LT BACKUP",
                "LT FRUITS PRIME",
                "LOST SCRIPT EZ"
            }

            -- Iniciar o loop
            spawn(function()
                while SpamClanYK2Active do
                    pcall(function()
                        -- Seleciona um nome de clÃ£ aleatÃ³rio
                        local selectedClanName = clanNames[math.random(1, #clanNames)]

                        -- Cria o clÃ£
                        game:GetService("ReplicatedStorage").Events.ClanEvent:FireServer({
                            ["clanToCreate"] = selectedClanName,
                            ["action"] = "create_clan",
                            ["ClanIcon"] = "70948224300476"
                        })

                        -- Convida todos os jogadores do servidor
                        for _, player in pairs(game.Players:GetPlayers()) do
                            game:GetService("ReplicatedStorage").invitationEvent:FireServer({
                                ["action"] = "invite_clan",
                                ["oplr"] = player
                            })
                        end
                    end)
                    wait() -- Intervalo entre cada ciclo (ajustÃ¡vel)
                end
            end)
        end
    end
})

-- Criando o Toggle no Orion
troll:AddToggle({
    Name = "🎶 Play Sounds for All",
    Default = false, -- Inicialmente desativado
    Callback = function(Value)
        if Value then
            isActive = true -- Ativa o estado do toggle

            -- FunÃ§Ã£o para tocar os sons sequencialmente
            local function playSequentialSounds()
                local replicatedStorage = game:GetService("ReplicatedStorage")

                -- Lista de IDs dos sons
                local soundIds = {
                    "7764369437",
                    "6929403333",
                    "6129291390",
                    "291315892"
                }

                -- Loop de sons enquanto o toggle estiver ativado
                while isActive do
                    for _, soundId in ipairs(soundIds) do
                        if not isActive then break end -- Interrompe o loop se o toggle for desativado
                        replicatedStorage.Events.PLAYEvent:FireServer(soundId)
                        wait(20) -- Aguarda 20 segundos antes de tocar o prÃ³ximo som
                    end
                end
            end

            -- Executa a funÃ§Ã£o em uma nova thread
            task.spawn(playSequentialSounds)
        else
            isActive = false -- Desativa o estado do toggle

            -- Para todos os sons
            game:GetService("ReplicatedStorage").Events.STOPEvent:FireServer()
        end
    end
})

