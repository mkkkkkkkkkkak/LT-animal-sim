textBox.Position = UDim2.new(0.05, 0, 0.2, 0)
textBox.Font = Enum.Font.GothamBold
textBox.TextColor3 = Color3.new(1, 1, 1)
textBox.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
textBox.Text = ""
textBox.TextSize = 14
Instance.new("UICorner", textBox)

local playBtn = Instance.new("TextButton", box)
playBtn.Text = "🎵 Play Song"
playBtn.Size = UDim2.new(0.9, 0, 0, 30) -- menor altura
playBtn.Position = UDim2.new(0.05, 0, 0.65, 0) -- ajustado para baixo no frame menor
playBtn.BackgroundColor3 = Color3.fromRGB(255, 210, 0)
playBtn.Font = Enum.Font.GothamBold
playBtn.TextColor3 = Color3.fromRGB(0, 0, 0)
playBtn.TextSize = 14
Instance.new("UICorner", playBtn)

            playBtn.MouseButton1Click:Connect(function()
                local songId = textBox.Text
                game:GetService("ReplicatedStorage").Events.PLAYEvent:FireServer(songId)
            end)
        end)
    end
})
