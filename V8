local pvp = Window:MakeTab({
        Name = "pvp/kill",
        Icon = "rbxassetid://87265759657753",
        PremiumOnly = false
    })

local pvpSection = pvp:AddSection({
        Name = "Lt3 🚸"
    })

pvp:AddToggle({
    Name = "☠️ Farm Kill Me",
    Default = false,
    Callback = function(Value)
        FarmKillMe = Value

        if FarmKillMe then
            -- Toca o som quando o farm Ã© ativado
            local sound = Instance.new("Sound")
            sound.SoundId = "rbxassetid://7269900245"
            sound.Parent = game.Players.LocalPlayer.Character.HumanoidRootPart
            sound:Play()

            spawn(function()
                while FarmKillMe do
                    pcall(function()
                        -- ObtÃ©m o personagem do jogador que ativou a opÃ§Ã£o
                        local player = game.Players.LocalPlayer
                        local character = player.Character

                        if character and character:FindFirstChild("HumanoidRootPart") then
                            -- Teleporta o jogador para o Dummy2
                            local dummy2 = workspace.MAP["5k_dummies"]:FindFirstChild("Dummy2")
                            if dummy2 and dummy2:FindFirstChild("HumanoidRootPart") then
                                character.HumanoidRootPart.CFrame = dummy2.HumanoidRootPart.CFrame
                            end

                            -- DÃ¡ dano no prÃ³prio jogador
                            local humanoid = character:FindFirstChild("Humanoid")
                            if humanoid then
                                -- Dispara o evento de dano no prÃ³prio jogador
                                game:GetService("ReplicatedStorage").jdskhfsIIIllliiIIIdchgdIiIIIlIlIli:FireServer(humanoid, 1)
                            end

                            -- Executa os TitleEvents em loop
                            game:GetService("ReplicatedStorage").Events.TitleEvent:FireServer("NoviceHunter")
                            game:GetService("ReplicatedStorage").Events.TitleEvent:FireServer("None")
                        end
                    end)
                    wait() -- MantÃ©m o delay
                end
            end)
        end
    end
})

pvp:AddToggle({
    Name = "☠️ Kill Players",
    Default = false,
    Callback = function(Value)
        KillPlayers = Value

        if KillPlayers then
            local sound = Instance.new("Sound")
            sound.SoundId = "rbxassetid://7269900245"  -- Som de kill (exemplo)
            sound.Parent = game.Players.LocalPlayer.Character.HumanoidRootPart
            sound:Play()

            spawn(function()
                while KillPlayers do
                    pcall(function()
                        -- Filtra os jogadores que nÃ£o sÃ£o o prÃ³prio jogador
                        local validPlayers = {}
                        for _, player in pairs(game.Players:GetPlayers()) do
                            if player ~= game.Players.LocalPlayer and player.Character and player.Character:FindFirstChild("Humanoid") then
                                table.insert(validPlayers, player)
                            end
                        end

                        if #validPlayers > 0 then
                            local randomPlayer = validPlayers[math.random(1, #validPlayers)]
                            -- Teleporta para o prÃ³ximo jogador aleatÃ³rio
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = randomPlayer.Character.HumanoidRootPart.CFrame

                            -- Aplica o dano ao jogador teleportado
                            game:GetService("ReplicatedStorage").jdskhfsIIIllliiIIIdchgdIiIIIlIlIli:FireServer(randomPlayer.Character.Humanoid, 3)

                            -- Aplica o dano no kill aura para jogadores dentro do raio de 50 unidades
                            for _, player in pairs(game.Players:GetPlayers()) do
                                if player ~= game.Players.LocalPlayer and player.Character and player.Character:FindFirstChild("Humanoid") then
                                    local distance = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - player.Character.HumanoidRootPart.Position).Magnitude
                                    -- Aplica o dano se estiver dentro do raio de 50 unidades
                                    if distance <= 50 then
                                        game:GetService("ReplicatedStorage").jdskhfsIIIllliiIIIdchgdIiIIIlIlIli:FireServer(player.Character.Humanoid, 3)
                                    end
                                end
                            end
                        end
                    end)
                    wait() -- Troca de jogador a cada 0.5 segundos
                end
            end)
        end
    end
})

local isTransforming = false
local connections = {}
local characterAddedConnection

-- Toggle 1: Istant respawn
pvp:AddToggle({
    Name = "⚜️ Istant respawn",
    CurrentValue = false, -- use CurrentValue e não Default
    Callback = function(Value)
        isTransforming = Value

        local Players = game:GetService("Players")
        local ReplicatedStorage = game:GetService("ReplicatedStorage")
        local player = Players.LocalPlayer

        local lastPosition = nil
        local transformed = false

        local function debugLog(...)
            print("[DEBUG]", ...)
        end

        local function forceReset()
            if transformed or not isTransforming then return end
            transformed = true
            debugLog("Triggering transformation")

            local character = player.Character
            if character then
                local rootPart = character:FindFirstChild("HumanoidRootPart")
                if rootPart then
                    lastPosition = rootPart.CFrame
                    debugLog("Saved death position:", lastPosition)
                end
            end

            ReplicatedStorage:WaitForChild("Events"):WaitForChild("SpawnEvent"):FireServer("monkey", "monkey6", "monkeyAnim")
            debugLog("Fired transformation event")

            task.wait(0.1)
            player:LoadCharacter()
        end

        local function cleanupConnections()
            for _, conn in ipairs(connections) do
                if conn then conn:Disconnect() end
            end
            connections = {}
        end

        local function onCharacterAdded(character)
            transformed = false
            if not isTransforming then return end
            debugLog("Character added:", character.Name)

            task.spawn(function()
                local rootPart = character:WaitForChild("HumanoidRootPart", 3)
                if lastPosition and rootPart then
                    rootPart.CFrame = lastPosition
                    lastPosition = nil
                    debugLog("Position restored")
                end
            end)

            task.spawn(function()
                local humanoid = character:WaitForChild("Humanoid", 3)
                if humanoid then
                    table.insert(connections, humanoid.HealthChanged:Connect(function(health)
                        if health <= 0 then
                            debugLog("Health is 0")
                            forceReset()
                        end
                    end))
                    table.insert(connections, humanoid.Died:Connect(function()
                        debugLog("Humanoid died")
                        forceReset()
                    end))
                else
                    debugLog("Humanoid not found")
                end
            end)
        end

        if isTransforming then
            characterAddedConnection = player.CharacterAdded:Connect(onCharacterAdded)
            if player.Character then
                onCharacterAdded(player.Character)
            end
        else
            cleanupConnections()
            if characterAddedConnection then
                characterAddedConnection:Disconnect()
                characterAddedConnection = nil
            end
            debugLog("Transformation disabled")
        end
    end,
})

local autoHeal = false
local healLoop

-- Toggle 2: Auto Heal
pvp:AddToggle({
    Name = "🍖 Auto Heal (Use animal)",
    CurrentValue = false,
    Callback = function(Value)
        autoHeal = Value
        local Players = game:GetService("Players")
        local RunService = game:GetService("RunService")
        local player = Players.LocalPlayer
        local humanoid = player.Character and player.Character:FindFirstChild("Humanoid")

        local function useFood()
            local food = player.Backpack:FindFirstChild("Food")
            if food and humanoid then
                humanoid:EquipTool(food)
                repeat
                    food:Activate()
                    task.wait(0.3)
                until humanoid.Health >= humanoid.MaxHealth or food.Parent ~= player.Character
            end
        end

        if autoHeal then
            healLoop = RunService.Heartbeat:Connect(function()
                if not humanoid or not humanoid.Parent then
                    player.CharacterAdded:Wait()
                    humanoid = player.Character:WaitForChild("Humanoid")
                end

                if humanoid and humanoid.Health < humanoid.MaxHealth then
                    useFood()
                end
            end)
        else
            if healLoop then
                healLoop:Disconnect()
                healLoop = nil
            end
        end
    end,
})

local pvpSection = pvp:AddSection({
        Name = "Look at the main tab for the tutorial."
    })

pvp:AddToggle({
    Name = "⚔️ close combat",
    CurrentValue = false,
    Callback = function(value)
        isActive = value
            local sound = Instance.new("Sound")
            sound.SoundId = "rbxassetid://7269900245"  -- Som de kill (exemplo)
            sound.Parent = game.Players.LocalPlayer.Character.HumanoidRootPart
            sound:Play()

        local Players = game:GetService("Players")
        local RunService = game:GetService("RunService")
        local ReplicatedStorage = game:GetService("ReplicatedStorage")
        local VirtualInputManager = game:GetService("VirtualInputManager")

        local player = Players.LocalPlayer
        local detectionDistance = 6

        if isActive then
            print("Auto PvP enabled")
            autoPVPLoop = RunService.RenderStepped:Connect(function()
                if not isActive then
                    if autoPVPLoop then
                        autoPVPLoop:Disconnect()
                        autoPVPLoop = nil
                    end
                    return
                end

                local character = player.Character
                local hrp = character and character:FindFirstChild("HumanoidRootPart")
                if not hrp then return end

                for _, otherPlayer in ipairs(Players:GetPlayers()) do
                    if otherPlayer ~= player and otherPlayer.Character then
                        local otherHRP = otherPlayer.Character:FindFirstChild("HumanoidRootPart")
                        if otherHRP then
                            local distance = (hrp.Position - otherHRP.Position).Magnitude
                            if distance <= detectionDistance then
                                VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.Q, false, game)
                                local args2 = {
                                    [1] = Vector3.new(otherHRP.Position.X, otherHRP.Position.Y, otherHRP.Position.Z),
                                    [2] = "NewFireball"
                                }
                                ReplicatedStorage:WaitForChild("SkillsInRS"):WaitForChild("RemoteEvent"):FireServer(unpack(args2))
                                task.wait(0.1)
                                break
                            end
                        end
                    end
                end
            end)
        else
            print("Auto PvP disabled")
            if autoPVPLoop then
                autoPVPLoop:Disconnect()
                autoPVPLoop = nil
            end
        end
    end
})

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local VirtualInputManager = game:GetService("VirtualInputManager")
local Cam = workspace.CurrentCamera
local LocalPlayer = Players.LocalPlayer

local renderConnection
local inputConnection
local FOVring

local fov = 110
local maxTransparency = 0.1
local detectionDistance = 6 -- ainda pode usar pra movimentação, se quiser

-- Função para atualizar posição do círculo no meio da tela
local function updateDrawings()
    if FOVring then
        FOVring.Position = Cam.ViewportSize / 2
    end
end

local function lookAt(targetPos)
    local lookVector = (targetPos - Cam.CFrame.Position).Unit
    Cam.CFrame = CFrame.new(Cam.CFrame.Position, Cam.CFrame.Position + lookVector)
end

local function calculateTransparency(distance)
    local transparency = (1 - (distance / fov)) * maxTransparency
    return math.clamp(transparency, 0, maxTransparency)
end

local function getPlayerInFOV(trg_part)
    local center = Cam.ViewportSize / 2
    for _, player in ipairs(Players:GetPlayers()) do
        if player ~= LocalPlayer then
            local part = player.Character and player.Character:FindFirstChild(trg_part)
            if part then
                local screenPos, onScreen = Cam:WorldToViewportPoint(part.Position)
                local distance = (Vector2.new(screenPos.X, screenPos.Y) - center).Magnitude
                if onScreen and distance <= fov then
                    return player
                end
            end
        end
    end
    return nil
end

local function moveToTarget(targetHRP)
    if not targetHRP then return end
    local humanoid = LocalPlayer.Character and LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
    if humanoid then
        humanoid:MoveTo(targetHRP.Position)
    end
end

local function onKeyDown(input)
    if input.KeyCode == Enum.KeyCode.Delete then
        pvp:Toggle("⚔️ Auto PvP (FOV)", false)
    end
end

pvp:AddToggle({
    Name = "⚔️ Auto PvP (FOV)",
    CurrentValue = false,
    Callback = function(state)
        if state then
            if not FOVring then
                FOVring = Drawing.new("Circle")
                FOVring.Visible = true
                FOVring.Thickness = 2
                FOVring.Color = Color3.fromRGB(128, 0, 128)
                FOVring.Filled = false
                FOVring.Radius = fov
                FOVring.Position = Cam.ViewportSize / 2
                FOVring.Transparency = maxTransparency
            end

            if not inputConnection then
                inputConnection = UserInputService.InputBegan:Connect(onKeyDown)
            end

            if not renderConnection then
                renderConnection = RunService.RenderStepped:Connect(function()
                    updateDrawings()
                    local targetPlayer = getPlayerInFOV("Head")

                    if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("Head") then
                        local targetHead = targetPlayer.Character.Head
                        lookAt(targetHead.Position)

                        local targetHRP = targetPlayer.Character:FindFirstChild("HumanoidRootPart")
                        moveToTarget(targetHRP)

                        -- Dispara o NewFireball SEM checar distância
                        VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.Q, false, game)
                        local args2 = {
                            [1] = targetHRP.Position,
                            [2] = "NewFireball"
                        }
                        ReplicatedStorage:WaitForChild("SkillsInRS"):WaitForChild("RemoteEvent"):FireServer(unpack(args2))

                        local screenPos, _ = Cam:WorldToViewportPoint(targetHead.Position)
                        local distScreen = (Vector2.new(screenPos.X, screenPos.Y) - (Cam.ViewportSize / 2)).Magnitude
                        FOVring.Transparency = calculateTransparency(distScreen)
                    else
                        FOVring.Transparency = maxTransparency
                        local humanoid = LocalPlayer.Character and LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
                        local hrp = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
                        if humanoid and hrp then
                            humanoid:MoveTo(hrp.Position)
                        end
                    end
                end)
            end
        else
            if renderConnection then
                renderConnection:Disconnect()
                renderConnection = nil
            end
            if inputConnection then
                inputConnection:Disconnect()
                inputConnection = nil
            end
            if FOVring then
                FOVring:Remove()
                FOVring = nil
            end
        end
    end
})

local autododge = false
local autoAttack = false
local attackLoop
local dodgeConnection
local skillConnection
local connections = {}

pvp:AddToggle({
    Name = "🏄🏻 Auto Dodge + Kill Aura",
    Default = false,
    Callback = function(value)
        local Players = game:GetService("Players")
        local ReplicatedStorage = game:GetService("ReplicatedStorage")
        local RunService = game:GetService("RunService")
        local LocalPlayer = Players.LocalPlayer

        autododge = value
        autoAttack = value

        local function getLocalHRP()
            return (LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()):WaitForChild("HumanoidRootPart")
        end

        local function teleportAwayFrom(attackerHRP, distance)
            local myHRP = getLocalHRP()
            if attackerHRP and myHRP then
                local safePos = attackerHRP.Position - attackerHRP.CFrame.LookVector * distance
                safePos = Vector3.new(safePos.X, safePos.Y + 3, safePos.Z)
                myHRP.CFrame = CFrame.new(safePos)
            end
        end

        local function monitorPlayersAndHealth()
            local myHRP = getLocalHRP()
            local myHumanoid = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid")
            if not myHRP or not myHumanoid then return end

            for _, player in pairs(Players:GetPlayers()) do
                if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                    local targetHRP = player.Character.HumanoidRootPart
                    local dist = (myHRP.Position - targetHRP.Position).Magnitude
                    if dist <= 8 then
                        teleportAwayFrom(targetHRP, 4)
                    end
                end
            end

            if myHumanoid.Health and myHumanoid.Health <= myHumanoid.MaxHealth * 0.5 then
                local backPos = myHRP.Position - myHRP.CFrame.LookVector * 6
                backPos = Vector3.new(backPos.X, backPos.Y + 3, backPos.Z)
                myHRP.CFrame = CFrame.new(backPos)
            end
        end

        local function onSkillFired(skillName, pos)
            if not autododge then return end
            if skillName == "NewFireball" then
                local myHRP = getLocalHRP()
                if myHRP and (pos - myHRP.Position).Magnitude <= 12 then
                    local safePos = myHRP.Position - (pos - myHRP.Position).Unit * 5
                    safePos = Vector3.new(safePos.X, safePos.Y + 3, safePos.Z)
                    myHRP.CFrame = CFrame.new(safePos)
                end
            end
        end

        -- Se ativar
        if value then
            -- Inicia loop de ataque (kill aura)
            if not attackLoop then
                attackLoop = task.spawn(function()
                    while autoAttack do
                        local closestPlayer
                        local shortestDistance = math.huge
                        local myChar = LocalPlayer.Character
                        if myChar and myChar:FindFirstChild("HumanoidRootPart") then
                            for _, player in pairs(Players:GetPlayers()) do
                                if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") and player.Character:FindFirstChild("Humanoid") then
                                    local distance = (myChar.HumanoidRootPart.Position - player.Character.HumanoidRootPart.Position).Magnitude
                                    if distance < shortestDistance and player.Character.Humanoid.Health > 0 then
                                        shortestDistance = distance
                                        closestPlayer = player
                                    end
                                end
                            end
                            if closestPlayer then
                                pcall(function()
                                    ReplicatedStorage:WaitForChild("jdskhfsIIIllliiIIIdchgdIiIIIlIlIli"):FireServer(closestPlayer.Character.Humanoid, 1)
                                end)
                            end
                        end
                        task.wait(0.1)
                    end
                end)
            end

            -- Conecta o dodge no RenderStepped se não conectado
            if not dodgeConnection then
                dodgeConnection = RunService.RenderStepped:Connect(monitorPlayersAndHealth)
                table.insert(connections, dodgeConnection)
            end

            -- Conecta o skill detection
            local remoteEvent = ReplicatedStorage:WaitForChild("SkillsInRS"):WaitForChild("RemoteEvent")
            if not skillConnection then
                skillConnection = remoteEvent.OnClientEvent:Connect(function(skillName, pos)
                    onSkillFired(skillName, pos)
                end)
                table.insert(connections, skillConnection)
            end

        -- Se desativar
        else
            -- Para os loops
            autoAttack = false
            autododge = false

            -- Cancela attackLoop
            if attackLoop then
                task.cancel(attackLoop)
                attackLoop = nil
            end

            -- Desconecta conexões de dodge e skill
            for _, conn in ipairs(connections) do
                if conn and conn.Disconnect then
                    conn:Disconnect()
                end
            end
            connections = {}

            dodgeConnection = nil
            skillConnection = nil
        end
    end
})

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

local LocalPlayer = Players.LocalPlayer

local aimbotFireball = false
local fireballLoop

local maxDistance = 140 -- distância maior agora

pvp:AddToggle({
    Name = "🔥 aimbot Fireball",
    CurrentValue = false,
    Callback = function(state)
        aimbotFireball = state
        
        if aimbotFireball then
            fireballLoop = RunService.Heartbeat:Connect(function()
                local character = LocalPlayer.Character
                if not character then return end
                local hrp = character:FindFirstChild("HumanoidRootPart")
                if not hrp then return end
                
                local closestPlayer = nil
                local shortestDistance = math.huge
                
                for _, player in pairs(Players:GetPlayers()) do
                    if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") and player.Character:FindFirstChild("Humanoid") and player.Character.Humanoid.Health > 0 then
                        local targetHRP = player.Character.HumanoidRootPart
                        local dist = (hrp.Position - targetHRP.Position).Magnitude
                        if dist < shortestDistance and dist <= maxDistance then
                            shortestDistance = dist
                            closestPlayer = player
                        end
                    end
                end
                
                if closestPlayer then
                    local targetHRP = closestPlayer.Character.HumanoidRootPart
                    local args = {
                        [1] = targetHRP.Position,
                        [2] = "NewFireball"
                    }
                    ReplicatedStorage:WaitForChild("SkillsInRS"):WaitForChild("RemoteEvent"):FireServer(unpack(args))
                end
            end)
        else
            if fireballLoop then
                fireballLoop:Disconnect()
                fireballLoop = nil
            end
        end
    end
})

local pvpSection = pvp:AddSection({
        Name = "t-1."
    })

pvp:AddToggle({
    Name = "🚷 Kill Aura",
    Default = false,
    Callback = function(Value)
        KillAura = Value
        
        if KillAura then
            local sound = Instance.new("Sound")
            sound.SoundId = "rbxassetid://7269900245"
            sound.Parent = game.Players.LocalPlayer.Character.HumanoidRootPart
            sound:Play()

            spawn(function()
                while KillAura do
                    pcall(function()
                        local player = game.Players.LocalPlayer
                        local replicatedStorage = game:GetService("ReplicatedStorage")

                        -- Aplica dano em players prÃ³ximos
                        for _, otherPlayer in pairs(game.Players:GetPlayers()) do
                            if otherPlayer ~= player and otherPlayer.Character and otherPlayer.Character:FindFirstChild("Humanoid") then
                                local distance = (otherPlayer.Character.HumanoidRootPart.Position - player.Character.HumanoidRootPart.Position).Magnitude
                                if distance <= 90 then
                                    replicatedStorage.jdskhfsIIIllliiIIIdchgdIiIIIlIlIli:FireServer(otherPlayer.Character.Humanoid)
                                end
                            end
                        end
                    end)
                    wait()  -- Adicionando uma pequena pausa para evitar sobrecarga
                end
            end)
        end
    end
})

pvp:AddToggle({
    Name = "🩸 Auto Retaliate",
    Default = false,
    Callback = function(Value)
        autoRetaliateActive = Value

        if autoRetaliateActive then
            -- Som ao ativar
            local sound = Instance.new("Sound")
            sound.SoundId = "rbxassetid://7269900245"
            sound.Parent = game.Players.LocalPlayer.Character.HumanoidRootPart
            sound:Play()

            -- ConfiguraÃ§Ã£o do retaliar dano
            local player = game.Players.LocalPlayer
            local character = player.Character or player.CharacterAdded:Wait()
            local humanoid = character:WaitForChild("Humanoid")
            local lastHealth = humanoid.Health

            -- Evento para verificar dano
            local function onHealthChanged(currentHealth)
                if currentHealth < lastHealth then
                    -- Encontra o atacante
                    local closestAttacker = nil
                    local closestDistance = math.huge

                    for _, otherPlayer in ipairs(game.Players:GetPlayers()) do
                        if otherPlayer ~= player then
                            local otherCharacter = otherPlayer.Character
                            if otherCharacter then
                                local otherHumanoid = otherCharacter:FindFirstChildOfClass("Humanoid")
                                local otherHRP = otherCharacter:FindFirstChild("HumanoidRootPart")
                                local myHRP = character:FindFirstChild("HumanoidRootPart")

                                if otherHumanoid and otherHRP and myHRP then
                                    local distance = (otherHRP.Position - myHRP.Position).Magnitude
                                    local maxDistance = 50 -- ConfigurÃ¡vel

                                    if distance <= maxDistance then
                                        closestAttacker = otherHumanoid
                                        closestDistance = distance
                                    end
                                end
                            end
                        end
                    end

                    -- Aplica dano no atacante mais prÃ³ximo
                    if closestAttacker then
                        game:GetService("ReplicatedStorage").jdskhfsIIIllliiIIIdchgdIiIIIlIlIli:FireServer(closestAttacker, 10)
                    end
                end
                lastHealth = currentHealth
            end

            -- Conecta o evento
            humanoid.HealthChanged:Connect(onHealthChanged)

        else
            -- Caso desative, se desconecta da funÃ§Ã£o para nÃ£o aplicar mais dano
            if humanoid and humanoid.HealthChanged then
                humanoid.HealthChanged:Disconnect()
            end
        end
    end
})

